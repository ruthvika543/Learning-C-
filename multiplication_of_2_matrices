#include<stdio.h>
#include<math.h>

int main()
{
    int i,j,a,b,k,r[5][5];
    printf("enter the number of rows and columns\n");
    scanf("%d %d",&i,&j);
    int p[5][5];
    printf("enter the elements in the matrix\n");
    for (a=0; a<i; a++)
    {
        for (b=0; b<j; b++)
        {
            scanf("%d",&p[a][b]);
        }
    }

    int q[5][5];
    printf("enter the elements in the matrix\n");
    for (a=0; a<i; a++)
    {
        for (b=0; b<j; b++)
        {
            scanf("%d",&q[a][b]);
        }
    }

    for (a=0; a<i; a++)
    {
        for (b=0; b<j; b++)
        {
           r[a][b]=0;
           for (k=0; k<j; k++)
           {
             r[a][b]=p[a][k]*q[k][j];
           }
        }
    }
    printf("Multiplication of two matrices is:")
    for (a=0; a<i; a++)
    {
        for (b=0; b<j; b++)
        {
           printf("%d\t",r[a][b]);
        }
        printf("\n");
    }
    return 0;
}
